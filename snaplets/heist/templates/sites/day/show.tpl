<apply template="base">
  <h4>Hits for <date/> <span class="right"><a href="/site/${id}">back</a></span></h4>

  <table>
    <tr>
      <th>url</th>
      <th>method</th>
      <th>hits</th>
      <th>max</th>
      <th>min</th>
      <th>avg</th>
      <th>var</th>
    </tr>
    <days>
      <tr>
        <td><url/></td>
        <td><method/></td>
        <td><hits/></td>
        <td><max/>ms</td>
        <td><min/>ms</td>
        <td><avg/>ms</td>
        <td><var/>ms</td>
      </tr>
    </days>
  </table>
</apply>
