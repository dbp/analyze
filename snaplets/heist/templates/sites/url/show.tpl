<apply template="base">
  <h4>
    Last 30 days for <url/> <method/>
  </h4>

  <table>
  <tr>
    <th>date</th>
    <th>hits</th>
    <th>avg</th>
    <th>max</th>
    <th>min</th>
    <th>var</th>
  </tr>
  <visits>
    <tr>
      <td><date/></td>
      <td><hits/></td>
      <td><avg/>ms</td>
      <td><max/>ms</td>
      <td><min/>ms</td>
      <td><var/></td>
    </tr>
  </visits>
  </table>

</apply>
